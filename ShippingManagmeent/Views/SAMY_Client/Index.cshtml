@model IEnumerable<ShippingManagmeent.SAMY_Client>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClientName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClientLocation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CtrateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Updatedate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClientName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClientLocation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CtrateDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Updatedate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
